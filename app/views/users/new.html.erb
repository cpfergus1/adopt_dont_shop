<head>
  <%= csrf_meta_tags %>

  <%= stylesheet_link_tag    'application', media: 'all' %>
  <%= javascript_include_tag 'application' %>
</head>

<body>
<form action="/users" method="post">
  <input type="hidden" name="authenticity_token" value="<%= form_authenticity_token %>">
  <p>Enter a new User:</p>
  <label>Name:</label><input type='text' name='user[name]'/><br>
  <label>Address:</label><input type='text' name ='user[address]'><br>
  <label>City:</label><input type='text' name ='user[city]'/><br>
  <label>State:</label><input type='text' name ='user[state]'/><br>
  <label>Zip:</label><input type='text' name ='user[zip]'/><br>
  <label></label><input type='submit' value="Submit"/>
</form>
</body>
